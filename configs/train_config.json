{
  "dataset_path": "data/medium_house_prices.csv",
  "target_column": "SalePrice",
  "test_size": 0.2,
  "random_state": 42,
  "automl": {
    "enabled": true,
    "models": [
      {
        "name": "RandomForest",
        "params": {
          "n_estimators": 200,
          "max_depth": null
        }
      },
      {
        "name": "LinearRegression",
        "params": {}
      },
      {
        "name": "ElasticNet",
        "params": {
          "alpha": 0.1,
          "l1_ratio": 0.5
        }
      },
      {
        "name": "XGBoost",
        "params": {
          "n_estimators": 200,
          "max_depth": 4,
          "learning_rate": 0.1
        }
      },
      {
        "name": "LightGBM",
        "params": {
          "n_estimators": 200,
          "learning_rate": 0.1
        }
      }
    ]
  }
}
